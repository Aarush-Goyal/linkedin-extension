{
  "manifest_version": 2,
  "name": "Linkedin Extension",
  "author": "Aarush Goyal",
  "version": "0.0.1",
  "description": "Tired of tapping auto connect on Linkedin? Try this",
  "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/mynetwork/"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab"]
}
